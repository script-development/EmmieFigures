{
  "name": "rapp-presence",
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build && vite build --ssr",
    "server": "node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server",
    "lint": "eslint \"{pages,components}/**/*.{js,vue}\" --cache --format=json -o eslint.out",
    "lint:check": "eslint \"{pages, components}/**/*.{js,vue}\" --fix --cache ",
    "prettier:check": "prettier --check \"{pages, components}/**/*.{js,vue}\""
  },
  "dependencies": {
    "@types/p5": "^1.3.1",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/server-renderer": "^3.2.20",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "p5": "^1.4.0",
    "source-map-js": "^0.6.2",
    "vite": "2.6.7",
    "vite-plugin-ssr": "^0.3.31",
    "vue": "^3.2.20"
  },
  "devDependencies": {
    "eslint": "^8.4.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "2.5.1"
  }
}
