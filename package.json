{
    "type": "module",
    "name": "rapp-presence",
    "scripts": {
        "dev": "npm run server",
        "prod": "npm run build && npm run server:prod",
        "build": "vite build && vite build --ssr",
        "server": "node ./src",
        "server:prod": "cross-env NODE_ENV=production node ./src",
        "lint": "eslint \"src/**/*.{js,vue}\" --cache --format=json -o eslint.out",
        "lint:check": "eslint \"src/**/*.{js,vue}\" --fix --cache ",
        "prettier:check": "prettier --check \"{src}/**/*.{js,vue}\"",
        "test": "jest"
    },
    "dependencies": {
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/compiler-sfc": "^3.2.31",
        "@vue/server-renderer": "^3.2.36",
        "axios": "^0.26.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "node-cache": "^5.1.2",
        "vite": "2.9.9",
        "vite-plugin-ssr": "^0.3.60",
        "vite-plugin-windicss": "^1.8.3",
        "vue": "^3.2.31"
    },
    "devDependencies": {
        "eslint": "^8.16.0",
        "eslint-plugin-jest": "^26.1.3",
        "eslint-plugin-vue": "^9.0.1",
        "jest": "^27.5.1",
        "prettier": "2.6.2"
    }
}
