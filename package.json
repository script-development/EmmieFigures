{
    "type": "module",
    "name": "rapp-presence",
    "scripts": {
        "dev": "npm run server",
        "prod": "npm run build && npm run server:prod",
        "build": "vite build && vite build --ssr",
        "server": "node ./src",
        "server:prod": "cross-env NODE_ENV=production node ./src",
        "lint": "eslint \"{src,tests}/**/*.{js,vue}\" --format=json -o eslint.out",
        "lint:check": "eslint \"{src,tests}/**/*.{js,vue}\" --fix --cache ",
        "prettier": "prettier --write \"src/**/*.{js,vue}\"",
        "prettier:check": "prettier --check \"src/**/*.{js,vue}\"",
        "test": "jest"
    },
    "dependencies": {
        "@vitejs/plugin-vue": "^1.9.3",
        "@vue/compiler-sfc": "^3.2.20",
        "@vue/server-renderer": "^3.2.20",
        "axios": "^0.24.0",
        "cron": "^1.8.2",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "node-cache": "^5.1.2",
        "vite": "2.6.7",
        "vite-plugin-ssr": "^0.3.39",
        "vue": "^3.2.20"
    },
    "devDependencies": {
        "eslint": "^8.4.0",
        "eslint-plugin-jest": "^25.3.0",
        "eslint-plugin-vue": "^8.2.0",
        "jest": "^27.4.5",
        "prettier": "2.5.1",
        "vite-plugin-windicss": "^1.8.3",
        "windicss": "^3.5.1"
    }
}
