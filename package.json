{
  "name": "rapp-presence",
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build && vite build --ssr",
    "server": "node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server",
    "lint": "eslint \"{pages,components}/**/*.{js,vue, p5js}\" --cache --format=json -o eslint.out",
    "lint:check": "eslint \"{pages, components, p5js}/**/*.{js,vue}\" --fix --cache ",
    "prettier:check": "prettier --check \"{pages, components, p5js}/**/*.{js,vue}\"",
    "test": "jest"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/server-renderer": "^3.2.31",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "p5": "^1.4.1",
    "vite": "2.7.13",
    "vite-plugin-ssr": "^0.3.51",
    "vue": "^3.2.20"
  },
  "devDependencies": {
    "@types/p5": "^1.4.0",
    "eslint": "^8.8.0",
    "eslint-plugin-vue": "^8.5.0",
    "jest": "^27.4.7",
    "prettier": "2.5.1"
  }
}
